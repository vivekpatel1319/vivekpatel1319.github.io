  }; // send form contents to clickD (via our servelet)


  var sendClickD = function sendClickD(topicID) {
    console.log('send form now');
    var currentForm = document.querySelector(settings.incommingFormSelector); // console.log(currentForm);

    var FD = new FormData(currentForm); // for (var [key, value] of currentFormData.entries()) {
    //   console.log(key, value);
    // }

    var XHR = new XMLHttpRequest();
    XHR.overrideMimeType("application/x-www-form-urlencoded; charset=UTF-8");
    XHR.open('POST', settings.formServicePrefix, true);
    XHR.send(FD);
