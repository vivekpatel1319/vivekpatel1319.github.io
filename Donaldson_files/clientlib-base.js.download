    var highestPossiblePrimaryFlow = 0;
    var highestPossibleSecondaryFlow = 0;
    var primaryFlowDetails = null;
    var secondaryFlowDetails = null;
    tags.forEach(function (tag) {
      if (!tag.split("flow-rate-range/")[1]) {
        return;
      }

      if (tag.split("flow-rate-range/primary-filter/")[1]) {
        primaryFlowDetails = tag.split("flow-rate-range/primary-filter/")[1].split("-");
      } else if (tag.split("flow-rate-range/secondary-filter/")[1]) {
        secondaryFlowDetails = tag.split("flow-rate-range/secondary-filter/")[1].split("-");
      }

      if (primaryFlowDetails) {
        var bottomOfPrimaryTagRange = parseInt(primaryFlowDetails[0]);
        var topOfPrimaryTagRange = parseInt(primaryFlowDetails[1]);

        if (lowestPossiblePrimaryFlow > bottomOfPrimaryTagRange) {
