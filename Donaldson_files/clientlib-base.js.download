function copy(url) {
    console.log(url);
    var d = document.createElement('input');
    document.body.appendChild(d);
    d.value = window.location.origin + url;
    d.select();
    d.setSelectionRange(0, 99999);
    document.execCommand('copy');
    console.log('copied')
    document.body.removeChild(d);
}


/* adds a class for CSS animation triggered by scrolling into view. Polyfill required for IE11 */
function checkInView() {

  elsToCheck = document.querySelectorAll('.js-checkinview');
  
  var checkInViewObserver = new IntersectionObserver(function(section) {
    if (section[0].intersectionRatio <= 0) return;
    section[0].target.classList.add('js-checkinview--activated');
    // stop checking once found
    this.unobserve(section[0].target);
  });
    
  for (i = 0; i < elsToCheck.length; ++i) {
    var el = elsToCheck[i];
    checkInViewObserver.observe(el);
  }
}

function scrollTop() {
  $(window).on("resize scroll", function() {
    $(window).scrollTop() > 300 ? $("#scrollToTop").css({
        opacity: 1,
        "pointer-events": "auto"
    }) : $("#scrollToTop").css({
        opacity: 0,
        "pointer-events": "none"
    })
  });

  $("#scrollToTop").click(function(e) {
      e.preventDefault();
      var t = $(this).attr("href");
      $("html,body").animate({
          scrollTop: $(t).offset().top
      }, "slow")
  });
}

// Document ready kickoff
$(document).ready(function() {
  // add js class to body for (potential) style triggers
  $('body').addClass('js');
  // global make video embeds be responsive/scaling, add class to container
  $('.util-scaling-videos').fitVids();
  // global list item counting
  $('.js-listcount').each(function(){$(this).addClass('js-listcount--'+$(this).children().length)});
  // check for animation watchers
  checkInView();
  // scroll to top
  scrollTop();
  // init all the component js
  Util.hideIfEmpty(componentsToHideIfEmpty);
  for (var key in app.components) {
    app.components[key].init();
  };
});

