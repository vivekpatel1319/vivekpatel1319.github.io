      settings.placeHoldEl.innerHTML = responseHtml === undefined ? authoredData.noFormMessage : responseHtml;
      addFormTypeClass();
      makeMessageBoxesWide();
      limitMessageBoxLength();
      cleanupInlineJS();
      validateFieldsSetup();
      settings.spinnerEl.addEventListener('transitionend', function () {
        settings.placeHoldEl.classList.remove('opacityhidden');
        settings.formButton.parentNode.classList.remove('opacityhidden');
      }); // we need to know the topic in order to send to tracking on thanks page

      if (settings.dropDownEl != null) {
        settings.topicID = settings.dropDownEl.value;
      }

      if (settings.singleForm != null) {
        settings.topicID = settings.singleForm.dataset.singleformid;
      } //tracking


      var launchEventName = "xhr-form-viewed-contact-us-multi";

      if (settings.singleForm != null) {
        launchEventName = "xhr-form-viewed-contact-us-single";
      }

      window.appEventData.push({
        event: launchEventName,
        form: {
          transactionId: "",
          // unique id for that transaction
          topicFormId: settings.topicID
        }
      });
    } else {
      console.log('Form Service Problem');
      console.log(evt);
    }
  }; // What to do when either the request a form or the send form failed


  var transferFailed = function transferFailed(evt) {
