    incommingFormSelector: '#clickdimensionsForm',
    formButton: document.querySelector('#btnClickd'),
    validationNote: '<span class="clickd-contact-form__v-note"></span>',
    elements: [],
    requiredElements: [],
    vTally: 0,
    topicID: 0
  };
  var authoredData;

  if (settings.authoredData != null) {
    authoredData = {
      noFormMessage: settings.authoredData.dataset.noformerror,
      thanksPage: settings.authoredData.dataset.thankspage,
      requiredFieldMessage: settings.authoredData.dataset.requiredfield
    };
  } // Utility to add a class name unique to the form type for style purposes


  var addFormTypeClass = function addFormTypeClass() {
    var mainDiv = document.querySelector('.clickd-contact-form__place-hold .mainDiv');
    var formType = document.querySelector('.responsiveRow:first-of-type span') !== null ? document.querySelector('.responsiveRow:first-of-type span').textContent : 'not-specified';
    formType = formType.replace(/^\s+|\s+$/g, '');
    formType = formType.toLowerCase();
    formType = formType.replace(/[^a-z0-9 -]/g, '') // remove invalid chars
    .replace(/\s+/g, '-') // collapse whitespace and replace by -
    .replace(/-+/g, '-'); // collapse dashes

    mainDiv.classList.add('clickd-contact-form__type--' + formType);
  }; // Change the classes used for the message textarea so it can be full width


  var makeMessageBoxesWide = function makeMessageBoxesWide() {
    if (document.querySelector('.responsiveCell textarea') != null) {
      var textareaWrapper = document.querySelector('.responsiveCell textarea').parentNode.parentNode.parentNode;
      textareaWrapper.classList.remove('responsiveCellSize1');
      textareaWrapper.classList.remove('responsiveCellSize2');
      textareaWrapper.classList.add('responsiveCellSize3');
    }
  };

  var limitMessageBoxLength = function limitMessageBoxLength() {
    var firstTextarea = document.querySelector('.responsiveCell textarea');

    if (firstTextarea != null) {
      firstTextarea.setAttribute('maxlength', '700');
    }
  }; // Removing a ClickD inline js function which fails


  var cleanupInlineJS = function cleanupInlineJS() {
    // add in more cleaners here as needed
    var textareaEl = document.querySelector('.responsiveCell textarea');

    if (textareaEl != null) {
      textareaEl.removeAttribute('onkeyup');
    }
  }; // Validate the fields upon click into each


  var validateFields = function validateFields(currentFieldindex) {
    settings.elements = Array.from(document.querySelectorAll(settings.incommingFormSelector + " input:not([type='hidden']), " + settings.incommingFormSelector + " textarea")); //currentFieldindex === undefined ? console.log('%cNo Current Field', 'color:red') : console.log('%cCurrent Field = '+currentFieldindex , 'color:green');

    for (var i = currentFieldindex; i >= 0; i--) {
      if (settings.requiredElements.includes(settings.elements[i])) {
        var inputEl = settings.elements[i];

        if (inputEl.value == '' && currentFieldindex != i) {
          inputEl.parentNode.parentNode.parentNode.classList.add('clickd-contact-form__field--error');
        } else {
          inputEl.parentNode.parentNode.parentNode.classList.remove('clickd-contact-form__field--error');
        }
      }
    }
  }; // Validate all the fields


  var validateAllFields = function validateAllFields() {
    settings.vTally = 0;

    for (var i = settings.requiredElements.length; i > 0; i--) {
      t = i - 1;

      if (settings.requiredElements[t].value == '') {
        settings.vTally++;
      }
    }

    var isReady = settings.vTally < 1 ? true : false; // console.log(settings.vTally)
    // console.log(isReady);

    return isReady;
  };

  var validateFieldsSetup = function validateFieldsSetup() {
    settings.elements = Array.from(document.querySelectorAll(settings.incommingFormSelector + " input:not([type='hidden']), " + settings.incommingFormSelector + " textarea"));
    settings.requiredElements = settings.elements.filter(function (el) {
      return el.parentElement.querySelector("input[name='reqField']");
    });
    settings.vTally = settings.requiredElements.length;
    Array.prototype.forEach.call(settings.elements, function (el, i) {
      el.addEventListener('focus', function () {
        validateFields(i);
