  function getLowAndHighAirflowForProduct(listItem) {
    var tags = listItem.dataset.selectorTags.split(",");
    var lowestPossibleAirflow = 1501;
    var highestPossibleAirflow = 0;
    tags.forEach(function (tag) {
      if (!tag.split("airflow-range/")[1]) {
        return;
      }

      var airflowDetails = tag.split("airflow-range/")[1].split("-");
      var bottomOfTagRange = parseInt(airflowDetails[0]);
      var topOfTagRange = parseInt(airflowDetails[1]);

      if (lowestPossibleAirflow > bottomOfTagRange) {
        lowestPossibleAirflow = bottomOfTagRange;
      }

      if (highestPossibleAirflow < topOfTagRange) {
        highestPossibleAirflow = topOfTagRange;
      }
    });
    return {
      lowestPossibleAirflow: lowestPossibleAirflow,
      highestPossibleAirflow: highestPossibleAirflow
    };
  }

  function getLowAndHighFlowForProduct(listItem) {
