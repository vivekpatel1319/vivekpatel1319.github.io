function commaSeparateNumber(val) {
  while (/(\d+)(\d{3})/.test(val.toString())) {
    val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
  }

  return val;
}

function searchDonaldsonCatalog(query) {
  $(".search-term").text(query);
  var locale = window.location.pathname.replace(/^\/([^\/]*).*$/, '$1');

  if (locale == "en") {
    locale = "en-us";
  }

  var q = query.replace(" ", "+");
  var base_url = "//shop.donaldson.com/store/" + locale + "/aemsearch?Ntt=" + q + "&Ntk=All&Highlighting&locale=content";

  if (document.location.href.indexOf('//qa-') > 0) {
    base_url = base_url.replace('//shop', '//qa-shop');
  }

  var catalogRequest = $.ajax({
    url: base_url + "&format=json",
    method: "GET",
